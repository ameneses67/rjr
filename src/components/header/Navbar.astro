---
import { navMenu } from "../../data/navMenu";
---

<nav class="font-semibold text-slate-700 dark:text-slate-200">
  <ul class="flex space-x-12">
    {
      navMenu.map((item) => (
        <li>
          <a
            class="hover:text-indigo-500 dark:hover:text-indigo-400"
            href={item.linkURL}
          >
            {item.linkName}
          </a>
        </li>
      ))
    }
  </ul>
</nav>
